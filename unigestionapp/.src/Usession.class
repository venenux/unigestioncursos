Public Sub Form_Open()

Dim filas As New Variant[]
Dim titulos As New String[]

    FMain.Visible = False
    Usession.Raise
    
    usbtncerrar.Text = "Cerrar"
    usbtnactualizar.Text = "Cambiarlos"
    ustextuserinfo.Text = "Usuario: Usuer Devel, DocenteAdmin, Interno\nCreado: 20120621\nAlterado:20120622"
    
    titulos = ["fecha session", "detalles"]
    filas.Add(titulos, 0)
    filas.Add(["20160220", "lorem ipsum"], 1)
    filas.Add(["20170202", "data ipsum ber ber <br>veer"], 1)

    ususerdatagrid.setHeads = True           ' forzar la habilitacion
    ususerdatagrid.titles = titulos          ' entonces coloco los titulos, despues desabilitarlos
    ususerdatagrid.source = filas 

End

Public Sub usbtncerrar_Click()

    Usession_Close

End

Public Sub Usession_Close()

    FMain.Visible = True
    FMain.Raise
    Me.Close
    Return

End
